   "source": [
    "\n",
    "def my_rmse(y_true, y_pred):\n",
    "    error = y_true - y_pred \n",
    "    sqr_error = K.square(error)\n",
    "    mean_sqr_error = K.mean(sqr_error) \n",
    "    sqrt_mean_sqr_error = K.sqrt(mean_sqr_error) \n",
    "    return sqrt_mean_sqr_error"
   ]
